{
  "name": "@fuma-comment/server",
  "version": "0.5.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Fuma Nama",
  "homepage": "https://fuma-comment.vercel.app",
  "repository": "https://github.com/fuma-nama/fuma-comment",
  "exports": {
    ".": "./dist/index.js",
    "./express": {
      "import": "./dist/express/index.js",
      "require": "./dist/express/index.js",
      "types": "./dist/express/index.d.ts"
    },
    "./custom": {
      "import": "./dist/custom/index.js",
      "require": "./dist/custom/index.js",
      "types": "./dist/custom/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "express": [
        "./dist/express/index.d.ts"
      ],
      "custom": [
        "./dist/custom/index.d.ts"
      ]
    }
  },
  "files": [
    "./dist"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "build": "tsup",
    "test": "vitest run",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "eslint-config-custom": "workspace:*",
    "express": "^4.19.2",
    "tsconfig": "workspace:*",
    "tsup": "^8.2.3",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
